@article{taverna_polarized_2022,
	title = {Polarized x-rays from a magnetar},
	volume = {378},
	issn = {0036-8075, 1095-9203},
	url = {http://arxiv.org/abs/2205.08898},
	doi = {10.1126/science.add0080},
	abstract = {We report on the first detection of linearly polarized x-ray emission from an ultra-magnetized neutron star with the Imaging X-ray Polarimetry Explorer ({IXPE}). The {IXPE} 35 observations of the anomalous x-ray pulsar 4U 0142+61 reveal a linear polarization degree of \$(12{\textbackslash}pm 1){\textbackslash}\%\$ throughout the {IXPE} 2--8 {keV} band. We detect a substantial variation of the polarization with energy: the degree is \$(14{\textbackslash}pm 1){\textbackslash}\%\$ at 2--4 {keV} and \$(41{\textbackslash}pm 7){\textbackslash}\%\$ at 5.5--8 {keV}, while it drops below the instrumental sensitivity around 4--5 {keV}, where the polarization angle swings by \${\textbackslash}sim 90{\textasciicircum}{\textbackslash}circ\$. The {IXPE} observations give us completely new information about the properties of the neutron star surface and magnetosphere and lend further support to the presence of the quantum mechanical effect of vacuum birefringence.},
