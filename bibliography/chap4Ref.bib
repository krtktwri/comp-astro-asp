@misc{treves_magnificent_2001,
	title = {The Magnificent Seven: Close-by Cooling Neutron Stars?},
	url = {http://arxiv.org/abs/astro-ph/0011564},
	shorttitle = {The Magnificent Seven},
	abstract = {We model Galactic populations of accreting and cooling isolated neutron stars in the attempt to explore their link with a new class of dim soft X-ray sources revealed by {ROSAT}. For accretors we follow the magneto-rotational and dynamical evolution in the Galactic potential and a realistic large scale distribution of the interstellar medium is used. Under standard assumptions old neutron stars enter the accretor stage only if their magnetic field exceeds \${\textbackslash}approx 10{\textasciicircum}\{11\}\$--\$10{\textasciicircum}\{12\}\$ G. We predict about 1 source per square degree for fluxes \${\textbackslash}approx 10{\textasciicircum}\{-15\}\$--\$10{\textasciicircum}\{-16\}\$ erg cm\${\textasciicircum}\{-2\}\$s\${\textasciicircum}\{-1\}\$ in the energy range 0.5-2 {keV}. Cooling neutron stars are explored within a simpler model of local sources, including however interstellar absorption. They are found to be significantly less abundant at low fluxes, \${\textless}0.1\$ sources per square degree, but dominate over accretors at higher flux levels (\${\textbackslash}approx 10{\textasciicircum}\{-12\}\$--\$10{\textasciicircum}\{-11\}\$ erg cm\${\textasciicircum}\{-2\}\$s\${\textasciicircum}\{-1\}\$). We suggest that the faint sources observed by {ROSAT} may be young cooling neutron stars with typical age \${\textbackslash}la 10{\textasciicircum}6\$ yrs, if the total number of young neutron stars in the Solar proximity is \${\textbackslash}sim 10\$ times higher than inferred from radiopulsars statistics.},
	number = {{arXiv}:astro-ph/0011564},
	publisher = {{arXiv}},
	author = {Treves, A. and Popov, S. B. and Colpi, M. and Prokhorov, M. E. and Turolla, R.},
	urldate = {2023-05-01},
	date = {2001-12-30},
	eprinttype = {arxiv},
